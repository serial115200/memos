dt { font-weight: bold; }

table.layout { border-style: none; }

td.layout, th.layout { padding: 0.1em; border-style: none; }

pre { background-color: rgb(238, 238, 238); padding: 0.75em 1.5em; border: =
1px solid rgb(221, 221, 221); }

pre { white-space: pre-wrap; overflow-wrap: break-word; }

pre.newsforum { white-space: pre-wrap; overflow-wrap: break-word; }

kbd, code, file, pre samp, tt, var { font-family: "DejaVu Sans Mono", "Cour=
ier New", monospace, sans-serif; }

pre { font-size: 1.2em; }

kbd { font-weight: bold; }

kbd.cmd, kbd.option { font-weight: normal; }

code.const, code.var, code.fn { }

var.file, var.addr, var.option, var.cmd { font-style: normal; font-weight: =
normal; }

var.menu { font-style: italic; font-weight: normal; font-family: "DejaVu Sa=
ns Mono", "Courier New", monospace, sans-serif; }

blockquote.note { background-color: rgb(238, 238, 238); margin: 4em; paddin=
g: 0em 1em; border: 1px solid rgb(221, 221, 221); }

h3.note { text-align: center; }

p.footer { font-size: x-small; }

.lecturevideo { text-align: left; }

.lastyear { font-size: 0.65em; }

li.spaced { padding-bottom: 1em; }

.thai { font-size: 1.3em; }

.submitted { font-size: 0.8em; font-style: italic; }

.articledate { font-size: 50%; }

.pdflink { font-size: 70%; }

img.center { display: block; margin-left: auto; margin-right: auto; }

figcaption { font-style: italic; text-align: center; font-size: 0.9em; }

figure { text-align: center; }

header.sgordon { font-size: 80%; }

footer.sgordon { font-size: 80%; }

h1.sorted, h2.sorted, h3.sorted { }

